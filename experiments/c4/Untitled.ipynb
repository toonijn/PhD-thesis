   "source": [
    "from schrodinger import Schrodinger2D, Rectangle\n",
    "\n",
    "def V(x, y):\n",
    "    return x*x + y*y\n",
    "\n",
    "problem = Schrodinger2D(V, Rectangle(-10, 10, -10, 10), gridSize=[41,41], maxBasisSize=21)\n",
    "problem.eigenvalues(10)"
   ]
